    <script>

        function chatClick() {

            $('.chatbox-container').toggle(100);
            $('.chatbox-container').toggleClass('d-block');
            $('#chaticon').toggleClass('cross')
            $('.chatbox-body ul').removeClass('hide-rest-li');
            $('.p-message').hide();
            $('.chatbox-footer').hide();
            $('#chatmessage').text('');
            $('.form-control').removeClass('error-focus');
            $(".send-btn").removeClass('disable-btn');
            $('#hdnIsCalledTwice').val('');
            $('#txtChatboxQue').val('');
            //$('#chaticon').removeClass('d-block');
            // $("#chaticon").removeClass("far fa-comment-dots d-block").addClass("far fa-comment-dots");

        }


        $(".chatbox-body li a").click(function () {
            $(this).parent().addClass('selected').siblings().removeClass('selected');
            $('.chatbox-body ul').addClass('hide-rest-li');
            $('.p-message').show();
            $('.chatbox-footer').show();
            $('#chatmessage').hide();

            var _text = $(this).attr('id');
            var _message = '';
            $('#hdnQueryType').val(_text);

            switch (_text) {
                case 'Feedback':
                    _message = 'Please enter your valuable suggestions / feedback below.';
                    break;
                case 'Query':
                    _message = 'Please enter your queries below.';
                    break;
                case 'FAQ':
                    _message = 'Kindly see our <a href="faq.aspx" target="_blank">FAQs</a> for questions about using the website/app or a particular feature. If not found please enter your queries below.';
                    break;
            }
            $('#feedbackText').html(_message);

            //feedbackText
        });


        function postCommentNew(e) {
            if (e.keyCode == 13 && $(this).val()) {
                $('.form-control').removeClass('error-focus');
                if ($('#hdnIsCalledTwice').val() == '') {
                    $('#hdnIsCalledTwice').val(true);
                }
                if ($('#hdnIsCalledTwice').val() == 'true') {
                    $('#hdnIsCalledTwice').val(false);
                    $("#btnSend").click();
                }
                return false;
            } else if (e.keyCode == 13) {
                $('.form-control').addClass('error-focus');
                $(".chatbox-footer").show();
                return false;
            }
        }
    </script>

