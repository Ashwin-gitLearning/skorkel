    <script type="text/javascript">

        function AddNotificationCount() {
            var result = '';
            var NotificationId = document.getElementById("hdnNotificationcount").value;
            $.ajax({
                url: "handler/NotificationCount.ashx?NotificationId=" + NotificationId,
                dataType: "html",
                data: { method: 'ProcessRequest', args: { context: NotificationId } },
                success: function (data) {
                    if (data == 1) {
                        document.getElementById("lblNotifyCount").style.display = "none";
                        document.getElementById("divNotification1").style.display = "none";
                    }
                    else {
                        document.getElementById("lblNotifyCount").style.display = "none";
                        document.getElementById("divNotification1").style.display = "none";
                    }
                }
            });
        }

        $(document).ready(function () {

            $('#notificationBell').click(function (event) {
                event.stopPropagation();
                var callAddNot = false;
                if ($("#dvNotification").is(":hidden")) {
                    callAddNot = true;

                }
                $("#dvNotification").slideToggle("fast");
                if (callAddNot) {
                    AddNotificationCount();
                }
            });
            $("#dvNotification").on("click", function (event) {
                event.stopPropagation();
            });

            $('.chatbox-footer textarea').on('focus', function () {
                $(this).removeClass('error-focus');
            });
            // .addClass('input-desc-hover')


            $(document).on('click', '#btnSend', function () {
                var question = $('#txtChatboxQue').val();
                var type = $('#hdnQueryType').val();

                if (type == '') {
                    alert('Try again!');
                    return false;
                }
                if (question == '') {
                    //alert('Enter your ' + type + ' first!');

                    $('.form-control').addClass('error-focus');
                    $(".chatbox-footer").show();
                    return false;
                }
                else {

                    $('.form-control').removeClass('error-focus');
                }
                $.ajax({
                    type: "POST",
                    contentType: "application/json; charset=utf-8",
                    data: '{"question":"' + question + '","type":"' + type + '"}',
                    url: 'DocumentWebService.asmx/saveChatboxQuestion',
                    async: true,
                    dataType: "json",
                    beforeSend: function () {
                        $(".send-btn").addClass('send-btn disable-btn');
                    },
                    success: function (data) {
                        switch (data.d.toLowerCase()) {
                            case "feedback":
                                message = "Thanks for your feedback! Since Skorkel is a beta product, we look forward to incorporating your suggestions in our next release.";
                                break;
                            case "query":
                                message = "We apologize for the trouble you are facing. We will revert within 48 hours with a solution.";
                                break;
                            case "faq":
                                message = "Thanks for submitting your query. We will revert within 48 hours.";
                                break;
                            default:
                                message = string.Empty;
                                break;
                        }

                        $('#txtChatboxQue').val('');
                        $('#chatmessage').addClass('m-t-5 admin-message display-blockk padding-10');
                        $('#chatmessage').text(message);
                        $('.chat-loader').removeClass('d-flex');
                        $(".chatbox-footer").hide();
                    },
                    fail: function (xhr, textStatus, errorThrown) {
                        $(".send-btn").removeClass('disable-btn');
                    }
                });
            });
        });

        $(document).on("click", function () {
            $("#dvNotification").hide();
        });
    </script>

